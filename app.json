{
  "expo": {
    "name": "relaxfaceTres",
    "slug": "relaxfaceTres",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../PastaFotos/logo2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "../PastaFotos/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../PastaFotos/logo2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "../PastaFotos/logo2.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
